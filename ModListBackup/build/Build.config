<?xml version="1.0" encoding="UTF-8"?>
<Project DefaultTargets="BeforeBuild" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <UseDLLAssembly_CSharp>true</UseDLLAssembly_CSharp>
        <UseDLLAssembly_CSharp_firstpass>true</UseDLLAssembly_CSharp_firstpass>
        <UseDLLUnityEngine>true</UseDLLUnityEngine>

        <InstallToRimWorldModDir>true</InstallToRimWorldModDir>
        <SkipUnchangedOnCopy>true</SkipUnchangedOnCopy>
        <SilentMode>false</SilentMode>

        <ZipOutputOnDebug>false</ZipOutputOnDebug>
        <ZipOutputOnRelease>true</ZipOutputOnRelease>
    </PropertyGroup>
</Project>
